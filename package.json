{
  "name": "n8n-nodes-photocertif",
  "version": "1.2.1",
  "description": "PhotoCertif - Image/Photo and Art certification on Solana blockchain with AI authentication. Powered by CHECKHC.",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "photocertif",
    "checkhc",
    "blockchain",
    "solana",
    "nft",
    "certification",
    "image",
    "photo",
    "art",
    "ai-authentication",
    "art-certification",
    "image-verification"
  ],
  "license": "MIT",
  "homepage": "https://www.checkhc.net",
  "author": "CHECKHC <contact@checkhc.net> (https://checkhc.net)",
  "repository": {
    "type": "git",
    "url": "https://github.com/checkhc/n8n-nodes-photocertif.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier credentials nodes --write",
    "lint": "eslint 'credentials/**/*.ts' 'nodes/**/*.ts' package.json",
    "lintfix": "eslint 'credentials/**/*.ts' 'nodes/**/*.ts' package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "LICENSE",
    "SECURITY.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PhotoCertifApi.credentials.js",
      "dist/credentials/SolanaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PhotoCertif/PhotoCertif.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "eslint-config-riot": "^1.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  }
}
