{
  "name": "PhotoCertif - Art Certification (Irys/Arweave)",
  "nodes": [
    {
      "parameters": {},
      "id": "a1b2c3d4-e5f6-7890-abcd-ef1234567890",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "resourceType": "image2",
        "operation": "upload",
        "inputType": "url",
        "fileUrl": "https://example.com/artwork.jpg",
        "title": "My Artwork Title",
        "description": "Description of the artwork for certification"
      },
      "id": "b2c3d4e5-f6a7-8901-bcde-f12345678901",
      "name": "Upload Art Image",
      "type": "n8n-nodes-photocertif.photoCertif",
